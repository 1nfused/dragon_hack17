{
  "name": "Bot App Test",
  "version": "0.1.5",
  "manifest_version": 5,
  "description": "Yade yade",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Bot Test",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
          "http://*/*", "https://*/*"
      ],
      "js": [
        "js/floating.js",
        "js/socket.io.js",
        "js/script.js"
      ]
    }
],
"web_accessible_resources": ["js/floating.js", "js/socket.io.js", "js/script.js"],
"permissions": [
    "storage",
    "activeTab"
]
}