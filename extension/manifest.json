{
  "name": "Bot App Test",
  "version": "0.1.2",
  "manifest_version": 3,
  "description": "Yade yade",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Bot Test",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
          "http://*/*", "https://*/*"
      ],
      "js": [
        "floating.js",
        "script.js"
      ]
    }
],
"web_accessible_resources": ["floating.js", "script.js"],
"permissions": [
    "storage",
    "activeTab"
]
}